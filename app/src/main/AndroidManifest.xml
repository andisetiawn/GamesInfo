<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gamesinfo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GamesInfo">
        <activity
            android:name=".presentasion.activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentasion.activity.SettingActivity"
            android:exported="true"/>

        <activity
            android:name=".presentasion.activity.AboutActivity"
            android:exported="true"/>

        <activity
            android:name=".presentasion.activity.DetailActivity"
            android:exported="false"
            android:parentActivityName=".presentasion.activity.MainActivity"/>


    </application>

</manifest>